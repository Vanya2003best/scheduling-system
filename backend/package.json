{
  "name": "scheduling-system-backend",
  "version": "1.0.0",
  "description": "Backend API for the Employee Scheduling System",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand --detectOpenHandles",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": [
    "scheduling",
    "employee",
    "api",
    "express",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@types/date-fns": "^2.5.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "supertest": "^7.0.0"
  }
}
